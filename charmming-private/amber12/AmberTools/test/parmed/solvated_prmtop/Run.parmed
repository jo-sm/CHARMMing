#!/bin/sh

. ../../program_error.sh

if [ -z "$parmed" ]; then
   parmed="../../../bin/parmed.py"
fi

output=parmed.out
prmtop=solv.prmtop
prmtop2=CM_stripped.parm7.save

$parmed $prmtop parmed.in > $output 2>&1 || error
$parmed $prmtop2 parmed2.in >> $output 2>&1 || error

../../dacdif CM_Solute_ions.parm7.save CM_Solute_ions.parm7
../../dacdif CM_Solvent_ions.parm7.save CM_Solvent_ions.parm7
../../dacdif ions_solvent.parm7.save ions_solvent.parm7
../../dacdif CM_stripped.parm7.save CM_stripped.parm7
../../dacdif CM_stripped.rst7.save CM_stripped.rst7
../../dacdif receptor.prmtop.save receptor.prmtop
../../dacdif receptor.rst7.save receptor.rst7
../../dacdif receptor_excluded.prmtop.save receptor_excluded.prmtop

/bin/rm -f $output

exit 0
